<odoo>
    <record id="view_stock_picking_form_inherit_ruta" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.ruta</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_type']" position="after">
                <field name="route_id"/>
            </xpath>
        </field>
    </record>
</odoo>
